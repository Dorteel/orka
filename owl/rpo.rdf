        <rdfs:label>has accuracy</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/dorte/perception-ontology#hasMaximumValue -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/dorte/perception-ontology#hasMaximumValue">
