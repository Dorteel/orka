    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/dorte/perception-ontology#hasAbilityToObserve -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/dorte/perception-ontology#hasAbilityToObserve">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/dorte/perception-ontology#hasBoundingBox -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/dorte/perception-ontology#hasBoundingBox">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/dorte/perception-ontology#objectDetectionProperty"/>
    </owl:ObjectProperty>
    


