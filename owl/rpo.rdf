    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/dorte/perception-ontology#hasCertainty">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/dorte/perception-ontology#objectDetectionProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/dorte/perception-ontology#hasInput -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/dorte/perception-ontology#hasInput">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/dorte/perception-ontology#hasLocation -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/dorte/perception-ontology#hasLocation">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/dorte/perception-ontology#objectDetectionProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/dorte/perception-ontology#hasOutput -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/dorte/perception-ontology#hasOutput">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/dorte/perception-ontology#hasOutputUnitOfMeasure -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/dorte/perception-ontology#hasOutputUnitOfMeasure"/>
    


    <!-- http://www.semanticweb.org/dorte/perception-ontology#hasSensorCharacteristics -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/dorte/perception-ontology#hasSensorCharacteristics">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/dorte/perception-ontology#isSensorCharacteristicOf"/>
        <rdfs:label>has sensor charactertic</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/dorte/perception-ontology#hasUnitofMeasure -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/dorte/perception-ontology#hasUnitofMeasure">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/dorte/perception-ontology#Measurement"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/dorte/perception-ontology#SensorProperty"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://www.semanticweb.org/dorte/perception-ontology#UnitOFMeasure"/>
        <rdfs:comment>Decribes the unit of measure (quality) of the measurement</rdfs:comment>
        <rdfs:label>has unit of measure</rdfs:label>
        <owl:sameAs>http://www.ontology-of-units-of-measure.org/resource/om-2/hasUnit</owl:sameAs>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/dorte/perception-ontology#hosts -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/dorte/perception-ontology#hosts">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/dorte/perception-ontology#isHostedBy"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/dorte/perception-ontology#Agent"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/dorte/perception-ontology#Sensor"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/dorte/perception-ontology#isHostedBy -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/dorte/perception-ontology#isHostedBy">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/dorte/perception-ontology#Sensor"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/dorte/perception-ontology#Agent"/>
        <rdfs:label>is hosted by</rdfs:label>
        <owl:sameAs>http://www.w3.ord/ns/sosa/isHostedBy</owl:sameAs>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/dorte/perception-ontology#isSensorCharacteristicOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/dorte/perception-ontology#isSensorCharacteristicOf"/>
    


    <!-- http://www.semanticweb.org/dorte/perception-ontology#measuredBy -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/dorte/perception-ontology#measuredBy">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/dorte/perception-ontology#measuresProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/dorte/perception-ontology#measuresProperty -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/dorte/perception-ontology#measuresProperty">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/dorte/perception-ontology#objectDetectionProperty -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/dorte/perception-ontology#objectDetectionProperty">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/dorte/perception-ontology#observesProperty -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/dorte/perception-ontology#observesProperty">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/dorte/perception-ontology#operatesOn -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/dorte/perception-ontology#operatesOn"/>
    


