    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/dorte/perception-ontology#detectedBy -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/dorte/perception-ontology#detectedBy">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/dorte/perception-ontology#objectDetectionProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/dorte/perception-ontology#enablesObservationOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/dorte/perception-ontology#enablesObservationOf">
