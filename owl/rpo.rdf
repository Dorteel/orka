    <!-- http://www.semanticweb.org/dorte/perception-ontology#estimatedBy -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/dorte/perception-ontology#estimatedBy">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/dorte/perception-ontology#estimates -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/dorte/perception-ontology#estimates">
