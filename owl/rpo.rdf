        <rdfs:domain rdf:resource="http://www.semanticweb.org/dorte/perception-ontology#Sensor"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        <rdfs:label>is powered on</rdfs:label>
    </owl:DatatypeProperty>
    


