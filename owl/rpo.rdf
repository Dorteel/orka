        <rdfs:domain rdf:resource="http://www.semanticweb.org/dorte/perception-ontology#Measurement"/>
        <rdfs:label>has numerical value</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/dorte/perception-ontology#isPoweredOn -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/dorte/perception-ontology#isPoweredOn">
