        <rdfs:subClassOf rdf:resource="https://w3id.org/def/orka#Passive_Sensor"/>
        <rdfs:subClassOf rdf:resource="https://w3id.org/def/orka#Touch_Sensor"/>
        <rdfs:comment xml:lang="en">A sensor that measures changes in electrical resistance to detect touch or pressure. Examples include resistive touchscreens and pressure-sensitive resistors used in various applications.</rdfs:comment>
        <rdfs:label xml:lang="en">Proximity Sensor</rdfs:label>
    </owl:Class>
    


