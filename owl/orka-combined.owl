        <rdfs:subClassOf rdf:resource="https://w3id.org/def/orka#Exteroception_Sensor"/>
        <rdfs:subClassOf rdf:resource="https://w3id.org/def/orka#IdentificationSensor"/>
        <rdfs:comment>A technology that uses electromagnetic fields to automatically identify and track tags attached to objects. RFID tags are used in inventory tracking and access control systems.</rdfs:comment>
        <rdfs:label>RFID</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/def/orka#Ranging_Sensor -->

