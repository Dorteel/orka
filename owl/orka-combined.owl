        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.w3.org/ns/sosa/observes"/>
                <owl:allValuesFrom rdf:resource="https://w3id.org/def/orka#Angular_position"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>A variable resistor used to measure angular position.</rdfs:comment>
        <rdfs:label>Potentiometer</rdfs:label>
    </owl:Class>
    


