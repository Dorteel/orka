            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.w3.org/ns/sosa/observes"/>
                <owl:allValuesFrom>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://ecoinformatics.org/oboe/oboe.1.2/oboe-core.owl#characteristicFor"/>
                        <owl:allValuesFrom>
                            <owl:Class>
                                <owl:intersectionOf rdf:parseType="Collection">
                                    <rdf:Description rdf:about="https://w3id.org/def/orka#Robot"/>
                                    <owl:Restriction>
                                        <owl:onProperty rdf:resource="http://www.w3.org/ns/sosa/hosts"/>
                                        <owl:someValuesFrom rdf:resource="https://w3id.org/def/orka#ProprioceptorSensor"/>
                                    </owl:Restriction>
                                </owl:intersectionOf>
                            </owl:Class>
                        </owl:allValuesFrom>
                    </owl:Restriction>
                </owl:allValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>A class of sensors that measure the internal state of the robot, or parts of the robot</rdfs:comment>
        <rdfs:label>Proprioceptor Sensor</rdfs:label>
    </owl:Class>
    


