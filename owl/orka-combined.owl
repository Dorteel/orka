        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.w3.org/ns/sosa/observes"/>
                <owl:allValuesFrom rdf:resource="https://w3id.org/def/orka#Orientation"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">A sensor used to determine the orientation or direction of an object. Examples are gyroscopes and magnetic compasses used in navigation systems.</rdfs:comment>
        <rdfs:label xml:lang="en">Heading Sensor</rdfs:label>
    </owl:Class>
    


