        <rdfs:subClassOf rdf:resource="https://w3id.org/def/orka#Touch_Sensor"/>
        <rdfs:comment xml:lang="en">A sensor that measures the amount of force applied to an object. Examples include load cells used in weighing scales and force-sensitive resistors used in haptic devices.</rdfs:comment>
        <rdfs:label xml:lang="en">Force Sensor</rdfs:label>
    </owl:Class>
    


