        <rdfs:subClassOf rdf:resource="https://w3id.org/def/orka#Exteroception_Sensor"/>
        <rdfs:subClassOf rdf:resource="https://w3id.org/def/orka#Touch_Sensor"/>
        <rdfs:comment xml:lang="en">A sensor that detects the interruption of a light beam to determine the presence or absence of an object. Examples include photoelectric sensors used in security systems and safety light curtains in industrial automation.</rdfs:comment>
        <rdfs:label>Optical Barrier</rdfs:label>
    </owl:Class>
    


