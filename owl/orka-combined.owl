        <rdfs:subClassOf rdf:resource="https://w3id.org/def/orka#Motor_Sensor"/>
        <rdfs:comment>A sensor that measures position by detecting changes in inductance. Used in harsh environments for industrial applications.</rdfs:comment>
        <rdfs:label>Inductive Encoder</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/def/orka#Laser_Ranging -->

