sosa:ObservableProperty a owl:Class .

orka:Orientation a owl:Class ;
    rdfs:subClassOf orka:SpatialProperty .

orka:Speed_Sensor a owl:Class ;
    rdfs:label "Speed Sensor" ;
    rdfs:comment "Sensors that measures the speed of an object, such as speedometers and tachometers used in vehicles and machinery." ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty sosa:observes ;
            owl:someValuesFrom <http://ecoinformatics.org/oboe/oboe.1.2/oboe-characteristics.owl#Speed> ],
        sosa:Sensor .

orka:TemporalProperty a owl:Class ;
    rdfs:subClassOf orka:ObjectProperty .

orka:VisualCharactersitic a owl:Class ;
    rdfs:subClassOf orka:ObjectProperty .

orka:hasInputSize a owl:DatatypeProperty ;
    rdfs:label "has input size" ;
    rdfs:comment "Refers to the size of the image (in pixels) that is used as an input" ;
    rdfs:subPropertyOf orka:hasDeepLearningModelCharacteristic .

<http://www.semanticweb.org/mark/ontologies/2024/5/untitled-ontology-8/m> a swrl:Variable .

sosa:Procedure a owl:Class .

sosa:hosts a owl:ObjectProperty .

orka:MaterialProperty a owl:Class ;
    rdfs:subClassOf orka:ObjectProperty .

orka:Position_Sensor a owl:Class ;
    rdfs:label "Position Sensor" ;
    rdfs:comment "A sensor that detects the position of an object." ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom orka:Location ;
            owl:onProperty sosa:observes ],
        sosa:Sensor ;
    orka:hasWeBotsName "position_sensor" .

orka:ProprioceptorSensor a owl:Class ;
    rdfs:label "Proprioceptor Sensor" ;
    rdfs:comment "A class of sensors that measure the internal state of the robot, or parts of the robot" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom [ a owl:Restriction ;
                    owl:allValuesFrom [ a owl:Class ;
                            owl:intersectionOf ( orka:Robot [ a owl:Restriction ;
                                        owl:onProperty sosa:hosts ;
                                        owl:someValuesFrom orka:ProprioceptorSensor ] ) ] ;
                    owl:onProperty oboe-core:characteristicFor ] ;
            owl:onProperty sosa:observes ],
        sosa:Sensor .

orka:hasDeepLearningModelCharacteristic a owl:DatatypeProperty ;
    rdfs:domain orka:DeepLearningModel ;
    rdfs:subPropertyOf orka:hasAlgorithmCharacteristic .

orka:hasObservableCharacteristic a owl:ObjectProperty ;
    rdfs:subPropertyOf orka:hasCharacteristic .

orka:Distance_Sensor a owl:Class ;
    rdfs:label "Ranging Sensor" ;
    rdfs:comment """A sensor that measures the distance to an object.
For example, laser rangefinders and ultrasonic distance sensors used in robotics and surveying.""" ;
    rdfs:subClassOf sosa:Sensor,
        orka:Exteroceptor_Sensor ;
    skos:altLabel "Distance Sensor" ;
    orka:hasWeBotsName "distance_sensor" .

orka:IdentificationSensor a owl:Class ;
    rdfs:label "Identification Sensor" ;
    rdfs:comment "Sensors used to identify objects or individuals based on specific characteristics or signals. Examples are biometric scanners and RFID readers used in security and inventory management." ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty sosa:observes ;
            owl:someValuesFrom oboe-core:IdentifyingCharacteristic ],
        sosa:Sensor .

orka:ObjectProperty a owl:Class ;
    rdfs:subClassOf sosa:ObservableProperty .

orka:SizeProperty a owl:Class ;
    rdfs:subClassOf orka:GeometricProperty .

orka:Motor_Sensor a owl:Class ;
    rdfs:label "Motor Sensor" ;
    rdfs:comment """Sensors that monitor and measure various parameters of a motor's operation. Examples are encoders and tachometers used in industrial machinery to monitor motor speed and position.

TODO: Add motor as observed entity, and as part_of Robot""" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom orka:Location ;
            owl:onProperty sosa:observes ],
        sosa:Sensor,
        orka:ProprioceptorSensor .

orka:hasSensorCharacteristic a owl:DatatypeProperty .

orka:ComputerVisionAlgorithm a owl:Class ;
    rdfs:subClassOf sosa:Procedure .

orka:Touch_Sensor a owl:Class ;
    rdfs:label "Touch Sensor"@en ;
    rdfs:comment """A sensor that detects and measures physical contact or touch.
When these sensors activate, the activation implies the location of an entity at the range of the sensor. Examples include capacitive touchscreens, force-sensitive resistors, and tactile sensors used in robotics."""@en ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom orka:Location ;
            owl:onProperty sosa:observes ],
        [ a owl:Restriction ;
            owl:hasValue "0.0"^^xsd:float ;
            owl:onProperty orka:hasMeasurementRange ],
        sosa:Sensor,
        orka:Exteroceptor_Sensor ;
    orka:hasWeBotsName "touch_sensor" .

orka:Location a owl:Class ;
    rdfs:subClassOf orka:SpatialProperty ;
    owl:disjointWith orka:Orientation .

orka:YOLO a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:hasValue "MSCOCO" ;
            owl:onProperty orka:trainedOnDataSet ],
        orka:DeepLearningModel,
        orka:ObjectDetectionAlgorithm .

oboe-core:Entity a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty orka:hasCharacteristic ;
            owl:someValuesFrom oboe-core:Characteristic ],
        [ a owl:Restriction ;
            owl:onProperty orka:describedBy ;
            owl:someValuesFrom orka:Knowledge_Repository ] .

sosa:Sensor a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom sosa:ObservableProperty ;
            owl:onProperty sosa:observes ],
        ssn:System .

orka:Passive_Sensor a owl:Class ;
    rdfs:label "Passive Sensor"@en ;
    rdfs:comment "TODO: Power requirement needs to be further defined",
        "A sensor that does not require an external power source and relies on detecting energy emitted or reflected by objects."@en ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:hasValue "low" ;
            owl:onProperty orka:hasPowerRequirement ],
        sosa:Sensor .

orka:Active_Sensor a owl:Class ;
    rdfs:label "Active Sensor"@en ;
    rdfs:comment "TODO: Power requirement needs to be further defined",
        "A sensor that requires an external power source to operate and emits its own signal which interacts with the environment and measures the return signal."@en ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:hasValue "high" ;
            owl:onProperty orka:hasPowerRequirement ],
        sosa:Sensor .

sosa:observes a owl:ObjectProperty ;
    owl:equivalentProperty orka:observesCharacteristic ;
    owl:propertyChainAxiom ( sosa:madeObservation oboe-core:ofCharacteristic ) .

orka:Exteroceptor_Sensor a owl:Class ;
    rdfs:label "Exteroceptor Sensor"@en ;
    rdfs:comment "A sensor that provides information about the external environment surrounding the device."@en ;
    rdfs:subClassOf sosa:Sensor .

orka:PhysicalEntity a owl:Class ;
    rdfs:label "Physical Entity" ;
    rdfs:comment """Entities that have some form of physical existence. Used to denote entities that possess some characteristics that everyday objects possess (e.g. weight, color, size, etc.).
Despite the name, these entities can be virtual.""" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty orka:hasCharacteristic ;
            owl:someValuesFrom orka:Location ],
        oboe-core:Entity .

orka:inDataSet a owl:DatatypeProperty .

[] a swrl:Imp ;
    rdfs:label "Touch Sensor Detection" ;
    swrla:isRuleEnabled true ;
    rdfs:comment "If a touch sensor is activated, it means there is an entity with characteristic Location same as the location of the sensor itself" ;
    swrl:body [ a swrl:AtomList ;
            rdf:first [ a swrl:ClassAtom ;
                    swrl:argument1 <http://www.semanticweb.org/mark/ontologies/2024/5/untitled-ontology-8/sens> ;
                    swrl:classPredicate orka:Touch_Sensor ] ;
            rdf:rest [ a swrl:AtomList ;
                    rdf:first [ a swrl:IndividualPropertyAtom ;
                            swrl:argument1 <http://www.semanticweb.org/mark/ontologies/2024/5/untitled-ontology-8/sens> ;
                            swrl:argument2 <http://www.semanticweb.org/mark/ontologies/2024/5/untitled-ontology-8/loc> ;
                            swrl:propertyPredicate orka:hasLocation ] ;
                    rdf:rest [ a swrl:AtomList ;
                            rdf:first [ a swrl:IndividualPropertyAtom ;
                                    swrl:argument1 <http://www.semanticweb.org/mark/ontologies/2024/5/untitled-ontology-8/m> ;
                                    swrl:argument2 <http://www.semanticweb.org/mark/ontologies/2024/5/untitled-ontology-8/sens> ;
                                    swrl:propertyPredicate sosa:madeBySensor ] ;
                            rdf:rest [ a swrl:AtomList ;
                                    rdf:first [ a swrl:IndividualPropertyAtom ;
                                            swrl:argument1 <http://www.semanticweb.org/mark/ontologies/2024/5/untitled-ontology-8/o> ;
                                            swrl:argument2 <http://www.semanticweb.org/mark/ontologies/2024/5/untitled-ontology-8/m> ;
                                            swrl:propertyPredicate oboe-core:hasMeasurement ] ;
                                    rdf:rest [ a swrl:AtomList ;
                                            rdf:first [ a swrl:IndividualPropertyAtom ;
                                                    swrl:argument1 <http://www.semanticweb.org/mark/ontologies/2024/5/untitled-ontology-8/o> ;
                                                    swrl:argument2 <http://www.semanticweb.org/mark/ontologies/2024/5/untitled-ontology-8/e> ;
                                                    swrl:propertyPredicate oboe-core:ofEntity ] ;
                                            rdf:rest [ a swrl:AtomList ;
                                                    rdf:first [ a swrl:IndividualPropertyAtom ;
                                                            swrl:argument1 <http://www.semanticweb.org/mark/ontologies/2024/5/untitled-ontology-8/m> ;
                                                            swrl:argument2 <http://www.semanticweb.org/mark/ontologies/2024/5/untitled-ontology-8/c> ;
                                                            swrl:propertyPredicate oboe-core:ofCharacteristic ] ;
                                                    rdf:rest () ] ] ] ] ] ] ;
    swrl:head [ a swrl:AtomList ;
            rdf:first [ a swrl:ClassAtom ;
                    swrl:argument1 <http://www.semanticweb.org/mark/ontologies/2024/5/untitled-ontology-8/c> ;
                    swrl:classPredicate orka:Location ] ;
            rdf:rest [ a swrl:AtomList ;
                    rdf:first [ a swrl:IndividualPropertyAtom ;
                            swrl:argument1 <http://www.semanticweb.org/mark/ontologies/2024/5/untitled-ontology-8/e> ;
                            swrl:argument2 <http://www.semanticweb.org/mark/ontologies/2024/5/untitled-ontology-8/loc> ;
                            swrl:propertyPredicate orka:hasLocation ] ;
                    rdf:rest () ] ] .

[] a owl:AllDisjointClasses ;
