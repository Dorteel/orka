orka:RadianPerSecond a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty oboe-core:hasUnit ;
            owl:someValuesFrom <http://ecoinformatics.org/oboe/oboe.1.2/oboe-standards.owl#Radian> ],
        [ a owl:Restriction ;
            owl:onProperty oboe-core:hasUnit ;
            owl:someValuesFrom [ a owl:Class ;
                    owl:intersectionOf ( oboe-core:DerivedUnit [ a owl:Restriction ;
                                owl:allValuesFrom <http://ecoinformatics.org/oboe/oboe.1.2/oboe-standards.owl#Second> ;
                                owl:onProperty oboe-core:hasUnit ] [ a owl:Restriction ;
                                owl:hasValue -1 ;
                                owl:onProperty oboe-core:hasPower ] ) ] ],
        [ a owl:Restriction ;
            owl:allValuesFrom [ a owl:Class ;
                    owl:intersectionOf ( oboe-core:Measurement [ a owl:Restriction ;
                                owl:allValuesFrom orka:Orientation ;
                                owl:onProperty oboe-core:ofCharacteristic ] ) ] ;
            owl:onProperty oboe-core:standardFor ],
        oboe-core:CompositeUnit .

