orka:Light_Sensor a owl:Class ;
    rdfs:label "Light Sensor" ;
    rdfs:comment "Sensors that observe the presence of light above a certain threshold." ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom orka:Brightness ;
            owl:onProperty sosa:observes ],
        sosa:Sensor,
        orka:Exteroceptor_Sensor ;
    orka:hasWeBotsName "light_sensor" .

orka:Position a owl:Class ;
