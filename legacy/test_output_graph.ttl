orka:hasPowerRequirement a owl:DatatypeProperty ;
    rdfs:label "has power requirement" ;
    rdfs:comment "Used to indicate the power requirement of the sensor devices" ;
    rdfs:domain sosa:Sensor ;
    rdfs:range [ a rdfs:Datatype ;
            owl:oneOf [ a rdf:List ;
                    rdf:first "high" ;
                    rdf:rest [ a rdf:List ;
                            rdf:first "low" ;
                            rdf:rest () ] ] ] ;
    rdfs:subPropertyOf owl:topDataProperty .

orka:observedIn a owl:ObjectProperty ;
    rdfs:label "observed in" ;
    rdfs:comment "Relates an entity to an observation" ;
    rdfs:domain oboe-core:Entity ;
    rdfs:range oboe-core:Observation ;
    rdfs:subPropertyOf owl:topObjectProperty .

