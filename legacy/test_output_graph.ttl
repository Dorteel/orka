orka:Altitude_Sensor a owl:Class ;
    rdfs:label "Altitude Sensor" ;
    rdfs:comment """A sensory device that returns a measurement corresponding to the altitude the sensor is located at.

TODO: need to add sensors that can measure altitude based on the orientation of the sensor (e.g. distance sensor)""" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty sosa:observes ;
            owl:someValuesFrom orka:Altitude ],
        sosa:Sensor,
        orka:ProprioceptorSensor ;
    orka:hasWeBotsName "altimeter" .

orka:EnvironmentProperty a owl:Class ;
    rdfs:subClassOf sosa:ObservableProperty .

orka:Knowledge_Repository a owl:Class ;
    rdfs:label "Knowledge Repository" ;
    rdfs:comment "This class represents the external sources of knowledge ORKA can utilise such as WikiData and DBpedia." .

orka:Lidar a owl:Class ;
    rdfs:label "LiDAR" ;
    rdfs:comment "A sensor that measures distance by emitting a laser beam and calculating the time it takes for the beam to reflect back. Lidar sensors used in autonomous vehicles and mapping." ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom orka:Location ;
            owl:onProperty sosa:observes ],
        orka:Active_Sensor,
        orka:Exteroceptor_Sensor,
        orka:IdentificationSensor ;
    orka:hasWeBotsName "lidar" .

orka:Structure a owl:Class ;
    rdfs:subClassOf orka:GeometricProperty .

orka:ThermalProperty a owl:Class ;
    rdfs:subClassOf orka:ObjectProperty .

orka:canDetect a owl:ObjectProperty ;
    rdfs:label "can detect" ;
    rdfs:comment "Defines the relationship between an obejct detection algorithm and entities" ;
    rdfs:domain sosa:Procedure ;
    rdfs:range oboe-core:Entity .

orka:hasCharacteristic a owl:ObjectProperty ;
    rdfs:label "has characteristic" ;
    rdfs:comment "Links entities to the characteristics possessed by the entity" ;
    rdfs:domain oboe-core:Entity ;
    rdfs:range oboe-core:Characteristic ;
    rdfs:subPropertyOf owl:topObjectProperty ;
    owl:propertyChainAxiom ( orka:observedIn oboe-core:hasMeasurement oboe-core:ofCharacteristic ) .

orka:hasExternalDescription a owl:DatatypeProperty ;
    rdfs:label "has external description" .

orka:hasInferenceSpeed a owl:DatatypeProperty ;
    rdfs:subPropertyOf orka:hasDeepLearningModelCharacteristic .

orka:hasInferenceSpeedCPUb1 a owl:DatatypeProperty ;
    rdfs:comment "The inference speed on a CPU (Central Processing Unit) for a single image (batch size = 1), measured in milliseconds. It indicates how fast the model can process images on a standard computer without GPU acceleration" ;
    rdfs:subPropertyOf orka:hasInferenceSpeed .

orka:trainedOnDataSet a owl:DatatypeProperty .

