orka:GeometricProperty a owl:Class ;
    rdfs:subClassOf orka:VisualCharactersitic .

orka:Heading_Sensor a owl:Class ;
    rdfs:label "Heading Sensor"@en ;
    rdfs:comment "A sensor used to determine the orientation or direction of an object. Examples are gyroscopes and magnetic compasses used in navigation systems."@en ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom orka:Orientation ;
            owl:onProperty sosa:observes ],
