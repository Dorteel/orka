        <http://ecoinformatics.org/oboe/oboe.1.2/oboe-characteristics.owl>,
        <http://ecoinformatics.org/oboe/oboe.1.2/oboe-standards.owl>,
        <http://ecoinformatics.org/oboe/oboe.1.2/oboe.owl> ;
    owl:versionInfo 1.0 .

