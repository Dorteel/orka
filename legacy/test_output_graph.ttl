    owl:equivalentClass sosa:ObservableProperty .

oboe-core:IdentifyingCharacteristic a owl:Class .

oboe-core:ofCharacteristic a owl:ObjectProperty ;
    owl:equivalentProperty sosa:observedProperty,
        orka:ofCharacteristic .

